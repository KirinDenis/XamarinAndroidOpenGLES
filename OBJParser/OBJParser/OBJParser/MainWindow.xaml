<Window x:Class="OBJParser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OBJParser"
        mc:Ignorable="d"
        Title="Converter obj files" Height="255" Width="412" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        
        <Button Name="doParse"  Width="48" Height="30" Content="Convert" Click="doParse_Click"  HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,100,0,0"/>
        <Button Name="openFile"  Width="48" Height="30" Content="Open" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0" Click="OpenFile_Click"/>
        <TextBlock Name ="tb" Width="300" Height="50" Text="Test wrap Wrap test Text wrap Wrap Text" TextWrapping="Wrap" TextAlignment="Center" Margin="0,-120,0,0"/>
    </Grid>
</Window>
